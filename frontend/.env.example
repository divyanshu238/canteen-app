# ================================
# CANTEEN FRONTEND - ENVIRONMENT CONFIG
# ================================
# Copy this file to .env.local or .env.production.local

# Backend API URL
# Development: http://localhost:5000
# Production: https://your-backend-domain.com
VITE_API_URL=http://localhost:5000

# Razorpay Public Key ID (safe to expose to client)
# This is the KEY ID, not the secret!
# Get from: https://dashboard.razorpay.com/app/keys
# Use rzp_test_... for testing, rzp_live_... for production
VITE_RAZORPAY_KEY_ID=
